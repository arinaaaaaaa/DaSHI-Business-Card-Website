            "date": 1649363328695,
            "name": "Commit-0",
            "content": "import Header from \"../../components/universal/Header.js\"\nimport Footer from \"../../components/universal/Footer.js\";\nimport Question from \"../../components/universal/Question.js\";\nimport Menu from \"../../components/universal/LeftMenu.js\";\nimport Answer from \"../../components/universal/Answer.js\";\nimport styles from \"../../styles/Partners.module.css\";\nimport { AnswerList, FileList } from \"../../components/universal/Answer.js\";\n\nfunction PartnersInfo() {\n    return (\n        <div className={styles.partnersSection}>\n            <div className={styles.partnersHeader}>\n                <p className={styles.sectionTitle}>СТАТЬ ПАРТНЕРОМ <span style={{color: '#F46628'}}>DS</span></p>\n                <span className={styles.partnerTypes}>\n                    <button className=\"blueContainerOnHover\">DS +</button>\n                    <span className=\"tooltip\">\n                        <button style={{margin: 0}} disabled>DS Ultra</button>\n                        <span className=\"promt\">Раздел находится в разработке</span>\n                    </span>\n                </span>\n            </div>\n            <p className={styles.title}>Приглашаем Вас в нашу <span style={{color: '#F46628'}}>команду DS</span>!</p>\n            <div className=\"questionList\">\n                <Answer question=\"К чему приведет наща совместная работа?\" answer = {\n                    <>\n                        <p className=\"answerTitle\">Появится новая точка продаж DS в Вашем городе, населенном пункте. В результате:</p>\n                        <div className=\"list\">\n                            <AnswerList list={[\n                                \"Доступность необходимых запчастей\",\n                                \"Бесперебойная работа Вашей техники, отдела снабжения, отдела продаж\",\n                                \"Кратчайшие сроки поставки\",\n                                \"Доставка необходимой запчасти любым удобным способом в течение 1-2 дней\",\n                                \"Качественный и доступный сервис\",\n                                \"Мы предлагаем нашим представителям установку диагностического оборудования, в т.ч. мобильного, для оперативного выявления и устранения неточностей и возникших проблем\"\n                            ]}/>\n                        </div>\n                    </>\n                }/>\n                <Answer question=\"С чего начать?\" answer = {\n                    <>\n                        <p className=\"answerTitle\">Сделать это очень просто. Скачайте и заполните документы:</p>\n                        <FileList list={[\n                            [\"#\", \"Анкета «Анализ рынка в регионе»\"],\n                            [\"#\", \"Бланк «О продукции»\"],\n                            [\"#\", \"Анкета партнера с указанием желаемой формы сотрудничества*\"]\n                        ]}/>\n                        <p className=\"answerTitle\" style={{marginBottom: 0, marginTop: 29 + \"px\"}}>Заполненные документы можно отправить через <a href=\"#\" style={{color: '#F46628'}}>форму обратной связи</a> либо на <a href=\"#\" style={{color: '#F46628'}}>e-mail</a>.</p>\n                    </>\n                }/>\n                <Answer question=\"Формы сотрудничества\" answer = {\n                    <>\n                        <p className={styles.discription}><b>1. Покупатель DS</b> – работа по договору купли-продажи. Ваша организация либо индивидуально Вы закупаете необходимый товар с целью перепродажи либо размещения на складе и дальнейшей реализации. Работаем с НДС и без НДС.</p>\n                        <p className=\"answerTitle\" style={{marginBottom: 11 +\"px\"}}>От нас</p>\n                        <AnswerList list={[\n                            \"Выполнение всех принятых и прописанных в договоре обязательств\",\n                            \"Информирование о статусе товара – его местонахождении и стоимости, при отсутствии в наличии – о сроках поставки\",\n                            \"Оперативная доставка и отправка на Ваш адрес\",\n                            \"Постпродажное обслуживание\",\n                            \"Информирование о поступлении товара на склад\"\n                        ]}/>\n                        <p className=\"answerTitle\" style={{marginBottom: 11 +\"px\", marginTop: 24 + \"px\"}}>От вас</p>\n                        <AnswerList list={[\n                            \"Выполнение всех принятых и прописанных в договоре обязательств\",\n                            \"Актуальная информация при заказе – номер товара, количество, данные получателя\",\n                            \"Своевременное выполнение финансовых обязанностей\"\n                        ]}/>\n                        <p className={styles.discription} style={{marginTop: 56 + \"px\", marginBottom: 31 + \"px\"}}><b>2. Представитель DS - дилер.</b> Работаем по договору купли-продажи и заключаем допсоглашение о сотрудничестве.</p>\n                        <p className=\"answerTitle\">Преимущества данного вида сотрудничества:</p>\n                        <AnswerList list={[\n                            \"Вы получаете право использовать сертификат на ввоз продукции – Ваша организация может импортировать груз самостоятельно на территорию РФ\",\n                            \"Информация о Вашей компании добавляется на наш сайт в раздел Партнеры\"\n                        ]}/>\n                        <p className=\"answerTitle\" style={{marginBottom: 11 +\"px\", marginTop: 24 + \"px\"}}>От нас</p>\n                        <AnswerList list={[\n                            \"Предоставление ряда элементов фирменного стиля в электронном виде, презентации, рекламной продукции\",\n                            \"Выполнение всех принятых и прописанных в договоре обязательств\",\n                            \"Информирование о статусе товара – его местонахождении и стоимости, при отсутствии в наличии – о сроках поставки\",\n                            \"Оперативная доставка и отправка на Ваш адрес\",\n                            \"Постпродажное обслуживание\",\n                            \"Информирование о поступлении товара на склад\"\n                        ]}/>\n                        <p className=\"answerTitle\" style={{marginBottom: 11 +\"px\", marginTop: 24 + \"px\"}}>От вас</p>\n                        <AnswerList list={[\n                            \"Выполнение всех принятых и прописанных в договоре обязательств – определяется закупочный минимум:\",\n                            \"Периодичность закупа утверждается предварительно – ежемесячно / ежеквартально / раз в полгода;\",\n                            \"Объем закупа определяется в зависимости от местоположения и возможностей Вашего предприятия\",\n                            \"Предварительное информирование о закупочных планах – номер товара, количество, данные получателя\",\n                            \"Своевременное выполнение финансовых обязательств\",\n                            \"Своевременное информирование о ситуации на рынке в Вашей точке сбыта\"\n                        ]}/>\n                        <p className={styles.ps}>***Гарантированный минимальный объем закупа в месяц:</p>\n                        <ul className={styles.pps}>\n                            <li>форсунки - от ** шт.,</li>\n                            <li>комплектующие - не установлено.</li>\n                        </ul>\n                        <p className={styles.discription} style={{marginTop: 56 + \"px\"}}><b>3. Пакет &quot;DS+&quot; - региональный дистрибьютер.</b> Работа по договору купли-продажи, заключение договора о представлении бренда на закрепленной территории.</p>\n                        <p className=\"answerTitle\" style ={{marginBottom: 11 + \"px\", marginTop: 31 + \"xp\"}}>Преимущества пакета &quot;DS+&quot;:</p>\n                        <AnswerList list={[\n                            \"Закрепление определенной территории за Вашей организацией с последующим исключительным правом представлять продукцию DS на этой территории (населенный пункт, район, город, регион)\",\n                            \"Право использовать сертификат на ввоз продукции – Ваша организация может импортировать груз самостоятельно на территорию РФ\",\n                            \"Предоставление элементов фирменного стиля в электронном виде: гайдлайн – логобук, фирменные цвета, шрифты, паттерны, а также носителей фирменного стиля и сувенирной продукции\",\n                            \"Предоставление исходных файлов для дизайна сайта, концепции сайта и основных инструментов (в исходном значении)\",\n                            \"Разработка уникальной этикетки на упаковку\",\n                            \"Добавление Вашего предприятия на наш сайт в раздел Партнеры\",\n                            \"Индивидуальное ценообразование\",\n                            \"Внесение данных компании и продукции на Вашем складе в единую базу\",\n                            \"Рассрочка на диагностическое оборудование на срок до 3-х месяцев\"\n                        ]}/>\n                        <p className=\"answerTitle\" style={{marginBottom: 11 +\"px\", marginTop: 24 + \"px\"}}>От вас</p>\n                        <AnswerList list={[\n                            \"Продвижение и реализация продукции DS на установленной территории, в том числе с использованием предоставленных рекламных материалов\",\n                            \"Увеличение количества точек продаж и складов на установленной территории\",\n                            \"Своевременное информирование о ситуации на рынке в Вашей точке сбыта (анализ, отчетность, планирование)\",\n                            \"Выполнение всех принятых и прописанных в договоре обязательств – определяется закупочный минимум\",\n                            \"Периодичность закупа утверждается предварительно – ежемесячно / ежеквартально / раз в полгода\",\n                            \"Объем закупа определяется в зависимости от местоположения и возможностей Вашего предприятия\",\n                            \"Своевременное выполнение финансовых обязательств\"\n                        ]}/>\n                        <p className={styles.ps}>Минимальный объем закупа, месяц:</p>\n                        <ul className={styles.pps}>\n                            <li>форсунки - от ** шт.,</li>\n                            <li>комплектующие - от ** шт.</li>\n                        </ul>\n                    </>\n                }/>\n            </div>\n        </div>\n    )\n}\n\nfunction Partners() {\n    return (\n        <>\n            <Header/>\n            <Question/>\n            <div className=\"contentSection\">\n                <div className=\"content\">\n                    <Menu page='partners'/>\n                    <span>\n                        <PartnersInfo/>\n                        <img src=\"images/partnersImage.png\" alt=\"\" />\n                    </span>\n                </div>\n            </div>\n            <Footer/>\n        </>\n    )\n}\n\nexport default Partners;"
        }
    ]
}